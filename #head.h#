#ifndef NODE_H
#define NODE_H

#include<iostream>
#include<cstring>
#include"student.h"
class Node {
public:
  Node();//constructor
  ~Node();//destructor
  
  void setStudent(student* newStudent);//sets student
  student* getStudent();//returns student

  void setNext(Node* nNext);//sets next node
  Node* getNext();//return next node

private:
  student* studentPtr;//current student
  Node* next;//next node
};
#endif
